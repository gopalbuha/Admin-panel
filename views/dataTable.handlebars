</head>

<div class="main-panel">
    <div class="container">
        <div class="page-inner">
            <div class="page-header">
                <h4 class="page-title">DataTables.Net</h4>
                <ul class="breadcrumbs">
                    <li class="nav-home">
                        <a href="#">
                            <i class="flaticon-home"></i>
                        </a>
                    </li>
                    <li class="separator">
                        <i class="flaticon-right-arrow"></i>
                    </li>
                    <li class="nav-item">
                        <a href="#">Tables</a>
                    </li>
                    <li class="separator">
                        <i class="flaticon-right-arrow"></i>
                    </li>
                    <li class="nav-item">
                        <a href="#">Datatables</a>
                    </li>
                </ul>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">Basic</h4>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table action="dataTable" id="basic-datatables" class="display table table-striped table-hover">
                                    <thead>
                                        <tr>
                                             <th>id</th>
                                            <th>Category</th>
                                            <th>Update</th>
                                            <th>Delete</th>
                                            {{!-- <th>update</th>
                                            <th>delete</th> --}}
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th>id</th>
                                            <th>Category</th>
                                            <th>Update</th>
                                            <th>Delete</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                        {{#each categories}}
                                        <tr>

                                        <td>{{id}}</td>
                                        <td>{{Category}}</td>
                                        {{!-- <td>{{password}}</td> --}}
                                        <td><a href="categoryUpdate/{{id}}"><button>Edit</button></a></td>
                                        <td><button class="removeCategory" data-id="{{id}}">Delete</button></td>

                                        </tr>
                                        {{/each}}
                                        {{!-- <a href="/register"><button class="redbtn">Register</button></a> --}}
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>


                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">Multi Filter Select</h4>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table id="multi-filter-select" class="display table table-striped table-hover">
                                    <thead>
                                        <tr>
                                            <th>id</th>
                                            <th>Category</th>
                                            <th>Update</th>
                                            <th>Delete</th>
                                            {{!-- <th>update</th>
                                            <th>delete</th> --}}
                                        </tr>
                                    </thead>

                                    <tbody>

                                         {{#each categories}}
                                        <tr>
                                            <td>{{id}}</td>
                                        <td>{{Category}}</td>
                                        {{!-- <td>{{password}}</td> --}}
                                        <td><a href="categoryUpdate/{{id}}"><button>Edit</button></a></td>
                                        <td><button class="removeCategory" data-id="{{id}}">Delete</button></td>

                                        </tr>
                                        {{/each}}
                                        {{!-- <a href="/register"><button class="redbtn">Register</button></a> --}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <div class="d-flex align-items-center">
                                <h4 class="card-title">Add Row</h4>
                                <button class="btn btn-primary btn-round ml-auto" data-toggle="modal"
                                    data-target="#addRowModal">
                                    <i class="fa fa-plus"></i>
                                    Add Row
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

